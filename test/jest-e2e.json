{
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": "\\.e2e-spec\\.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "setupFiles": ["<rootDir>/set-test-env.ts"],
  "globalSetup": "<rootDir>/setup-e2e.ts",
  "testTimeout": 60000,
  "maxWorkers": 1
}
